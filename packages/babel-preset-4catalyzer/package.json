{
  "name": "@4c/babel-preset-4catalyzer",
  "version": "1.2.1",
  "description": "4C babel preset",
  "main": "index.js",
  "repository": "https://github.com/4Catalyzer/babel-preset-4catalyzer.git",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 8.3.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && node ./index.js",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "extends": ["@4c/semantic-release-config"]
  },
  "devDependencies": {
    "@4c/semantic-release-config": "^1.0.1",
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "eslint-config-4catalyzer": "^0.4.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.2",
    "prettier": "^1.11.1",
    "semantic-release": "^15.1.4",
    "travis-deploy-once": "^4.4.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.42",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-beta.42",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.0"

{
  "name": "@4c/babel-preset-4catalyzer",
  "version": "4.0.0",
  "description": "4C babel preset",
  "main": "index.js",
  "repository": "https://github.com/4Catalyzer/babel-preset-4catalyzer.git",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 8.3.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && jest --runInBand",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "extends": [
      "@4c/semantic-release-config"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@4c/semantic-release-config": "^1.0.2",
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "eslint-config-4catalyzer": "^0.4.4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest-cli": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "semantic-release": "^15.7.1",
    "travis-deploy-once": "^5.0.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-react-intl": "^2.3.0",
    "browserslist": "^4.2.1",
    "lodash": "^4.17.10"
  }
}

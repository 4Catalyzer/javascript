{
  "name": "semantic-release-alt-publish-dir",
  "version": "3.0.0",
  "main": "index.js",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 8.3.0"
  },
  "scripts": {
    "test": "jest --runInBand",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/semantic-release-alt-publish-dir.git"
  },
  "bugs": {
    "url": "https://github.com/jquense/semantic-release-alt-publish-dir/issues"
  },
  "homepage": "https://github.com/jquense/semantic-release-alt-publish-dir#readme",
  "description": "",
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "watchPathIgnorePatterns": [
      "fixtures"
    ]
  },
  "dependencies": {
    "@4c/file-butler": "^2.1.0",
    "cpy": "^7.0.0",
    "fs-extra": "^6.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.2",
    "@semantic-release/git": "^5.0.0",
    "@semantic-release/npm": "^3.3.1",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.5.0",
    "tempy": "^0.2.1",
    "travis-deploy-once": "^5.0.0"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "breaking",
          "release": "major"
        }
      ]
    },
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
